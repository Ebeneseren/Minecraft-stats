dictonary = {}
for i in range(len(UID)):
    dictonary[UID[i]] = Names[i




Userslist = os.listdir(r'C:\Users\aleks\Downloads\Minecraft\stats\stats')

Userslist = Userslist[0:-2]
play_time_user = {}

for i in range(len(Userslist)):
    Userstats = pd.read_json(Userslist[i])
    play_time= Userstats['stats']['minecraft:custom']['minecraft:play_time']/20/60/60
    username = dictonary[Userslist[i][0:-5]]
    play_time_user[username] = play_time
    
